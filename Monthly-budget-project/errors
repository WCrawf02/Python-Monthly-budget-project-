def calculate_expenses(budget_limit, expenses):
    total_expenses = 0
    for expense in expenses:
        total_expenses += expense
    return total_expenses

def check_budget(budget_limit, total_expenses):
    if total_expenses <= budget_limit
        print("You are within your budget.")
    else:
        print("You have exceeded your budget.")

def main():
    budget_limit = float(input("Enter your monthly budget limit: "))
    expenses = []
    num_expenses = int(input("Enter the number of expenses: "))
    for i in range(num_expenses):
        expense = float(input("Enter expense amount: "))
        expenses.append(expense)

    total_expenses = calculate_expenses(budget_limit, expenses)
    check_budget(budget_limit, total_expenses)

if __name__ == "__main__":
    main()
